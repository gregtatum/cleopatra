{
  "name": "cleopatra",
  "version": "0.0.1",
  "description": "Cleopatra, the gecko profiler UI",
  "main": "index.js",
  "scripts": {
    "test": "mocha --compilers js:babel-core/register test/**/*.js src/**/test/*.js",
    "start": "NODE_ENV=development node server.js",
    "start-no-hot": "NODE_ENV=development-no-hot node server.js",
    "start-prod": "npm run build-prod && npm run serve-static",
    "start-prod-readable": "npm run build-prod-readable && npm run serve-static",
    "serve-static": "http-server . -p 4242",
    "build": "NODE_ENV=development webpack --progress",
    "build-prod-readable": "NODE_ENV=production webpack --progress",
    "build-prod": "NODE_ENV=production webpack -p --progress",
    "eslint": "eslint index.js src",
    "eslint-fix": "eslint --fix index.js src",
    "documentation": "documentation"
  },
  "author": "Markus Stange <mstange@themasta.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mstange/cleopatra.git"
  },
  "devDependencies": {
    "array-move": "^1.0.0",
    "babel-cli": "^6.2.0",
    "babel-core": "^6.2.1",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.2.0",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babelify": "^7.2.0",
    "bisection": "0.0.3",
    "browserify": "^13.0.1",
    "chai": "^3.5.0",
    "clamp": "^1.0.1",
    "classnames": "^2.2.5",
    "documentation": "^4.0.0-beta2",
    "eslint": "^3.7.1",
    "eslint-config-google": "^0.6.0",
    "eslint-plugin-react": "^6.4.0",
    "express": "^4.13.4",
    "http-server": "^0.9.0",
    "memoize-immutable": "^2.1.0",
    "memoizesync": "^0.5.0",
    "mocha": "^3.1.1",
    "npm-run-all": "^3.1.0",
    "raw-loader": "^0.5.1",
    "react": "^15.0.1",
    "react-addons-shallow-compare": "^15.0.1",
    "react-dom": "^15.0.1",
    "react-hot-loader": "^3.0.0-beta.0",
    "react-redux": "^4.4.2",
    "react-router": "^2.4.1",
    "react-router-redux": "^4.0.5",
    "react-virtualized": "^8.0.12",
    "redux": "^3.4.0",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.0.1",
    "reselect": "^2.5.1",
    "shallowequal": "^0.2.2",
    "style-loader": "^0.13.1",
    "uglify": "^0.1.5",
    "uglify-js": "^2.6.1",
    "watchify": "^3.6.1",
    "webpack": "^1.12.15",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-hot-middleware": "^2.10.0",
    "worker-loader": "^0.7.1"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ],
    "plugins": [
      "react-hot-loader/babel"
    ]
  }
}
